<!DOCTYPE html>
<html>
  <head>
    <title>PublishDataVo</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>

    <!-- JQuery -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.js"></script>

    <!-- Bootstrap -->
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    
    <!-- Bootstrap Select -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-select/1.6.3/js/bootstrap-select.min.js"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-select/1.6.3/css/bootstrap-select.min.css">

    <!-- Multi Select -->
    <script src="https://rawgit.com/lou/multi-select/master/js/jquery.multi-select.js"></script>
    <link rel="stylesheet" href="https://rawgit.com/lou/multi-select/master/css/multi-select.css">
  </head>

  <body>
  <%= render 'layouts/navbar' %>
    <% if notice %>
    <p class="alert alert-success" align="center"><%= notice %></p>
    <% elsif alert %>
    <p class="alert alert-danger" align="center"><%= alert %></p>
    <% end %>

  	<div class='container-fluid'>
      <div class="panel panel-default col-xs-10 col-xs-offset-1">
        <div class="panel-body">
          <br />

  		    <%= yield %>
          
        </div>
      </div>
  	</div>
  </body>
</html>
